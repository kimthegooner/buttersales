    "mailparser": "^3.9.3",
    "next": "^14.2.35",
    "nodemailer": "^8.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.2.0",
    "@types/mailparser": "^3.4.6",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.1.0",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.0",

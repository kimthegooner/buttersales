  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "electron:dev": "concurrently -k \"next dev -p 3000\" \"wait-on http://127.0.0.1:3000 && electron .\"",
